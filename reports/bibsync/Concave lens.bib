eprint = {1604.01321},
issn = {0001-4966},
journal = {The Journal of the Acoustical Society of America},
keywords = {backward lamb mode,laser ultrasonics,negative reflection,phase conjugation},
number = {1},
pages = {591--600},
title = {{Negative reflection of Lamb waves at a free edge: Tunable focusing and mimicking phase conjugation}},
url = {http://arxiv.org/abs/1604.01321{\%}5Cnhttp://scitation.aip.org/content/asa/journal/jasa/140/1/10.1121/1.4959024 http://dx.doi.org/10.1121/1.4959024},
