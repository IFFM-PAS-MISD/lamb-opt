@article{Salowitz2013,
abstract = {The human skin hosts an array of sensors that are capable of detecting and interpreting many traits important to how we function and survive. The goal of mimicking this capability in composites to create intelligent composite materials has led to the development of a bio-inspired stretchable network composed of numerous micro-fabricated sensors capable of detecting multiple stimuli. The components of the network are small scale and flexible making the network embeddable within complexly shaped composite layups and flexible structures with minimal impact on the host structure. This paper outlines recent progress in ongoing work to develop the bio-inspired network in order to create intelligent composite materials.},
author = {Salowitz, Nathan and Guo, Zhiqiang and Li, Yu Hung and Kim, Kyunglok and Lanzara, Giulia and Chang, Fu Kuo},
